{
  "name": "guestphotoupload",
  "version": "1.0.0",
  "description": "guest photo upload to google drive",
  "main": "index.js",
  "scripts": {
    "start": "node server.js",
    "worker": "node worker.js",
    "dev": "node server.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "docker:build": "docker build -t guestphotoupload-backend .",
